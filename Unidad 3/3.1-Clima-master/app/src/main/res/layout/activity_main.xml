<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.oscar.serviciosweb.MainActivity"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/BtnGen"
        android:layout_width="127dp"
        android:layout_height="51dp"
        android:layout_marginBottom="11dp"
        android:text="Clima"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/txtSalida"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/txtSalida"
        app:layout_constraintTop_toBottomOf="@+id/txtSalida" />

    <TextView
        android:id="@+id/txtSalida"
        android:layout_width="0dp"
        android:layout_height="230dp"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toTopOf="@+id/BtnGen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="198dp"
        android:layout_height="126dp"
        android:background="@drawable/clima"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BtnGen" />
</android.support.constraint.ConstraintLayout>
